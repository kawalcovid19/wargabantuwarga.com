    "netlify-plugin-image-optim": "0.4.0",
    "ora": "^5.4.1",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "tailwindcss": "2.2.4",
    "typescript": "4.3.5"
