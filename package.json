{
  "name": "wargabantuwarga.com",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap && ts-node etc/create-sitemap-index.ts",
    "dev": "next",
    "analyze": "cross-env ANALYZE=true yarn build",
    "fetch-wbw": "ts-node etc/fetchers/fetch-wbw.ts",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "netlify-export": "yarn fetch-wbw && yarn build && next export",
    "prepare": "husky install",
    "start": "next start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "jest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cypress:open",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "1.3.0",
    "@heroicons/react": "1.0.2",
    "@netlify/plugin-nextjs": "3.7.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "0.4.1",
    "cheerio": "^1.0.0-rc.10",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.1.4",
    "globby": "^11.0.4",
    "htmr": "^1.0.0",
    "itemsjs": "^2.1.10",
    "next": "11.0.1",
    "next-seo": "^4.26.0",
    "nprogress": "^0.2.0",
    "preact": "^10.5.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-debounce": "^3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@netlify/plugin-lighthouse": "2.1.2",
    "@next/bundle-analyzer": "^11.0.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.0",
    "@types/jest": "26.0.24",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.3.2",
    "@types/react": "17.0.13",
    "autoprefixer": "10.3.0",
    "chalk": "^4.1.1",
    "cloudinary-build-url": "^0.2.1",
    "critters": "^0.0.10",
    "cross-env": "^7.0.3",
    "cypress": "^7.7.0",
    "eslint": "7.30.0",
    "eslint-config-kentcdodds": "^19.1.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "frontmatter-markdown-loader": "3.6.3",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.0.1",
    "netlify-plugin-cache-nextjs": "1.6.1",
    "netlify-plugin-image-optim": "0.4.0",
    "next-sitemap": "^1.6.140",
    "ora": "^5.4.1",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "start-server-and-test": "^1.12.6",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "tailwindcss": "2.2.4",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "private": true
}
