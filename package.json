{
  "name": "wargabantuwarga.com",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "fetch-wbw": "node etc/fetchers/fetch-wbw.js",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
