    "cheerio": "^1.0.0-rc.10",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.1.4",
    "htmr": "^1.0.0",
    "itemsjs": "^2.1.10",
    "next": "11.0.1",
    "next-seo": "^4.26.0",
    "preact": "^10.5.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-virtual": "^2.8.0"
  },
  "devDependencies": {
    "@netlify/plugin-lighthouse": "2.1.2",
    "@types/react": "17.0.13",
    "autoprefixer": "10.3.0",
    "chalk": "^4.1.1",
    "critters": "^0.0.10",
    "eslint": "7.30.0",
